# Environment Configuration for Draw.io Live Editor
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control

# =============================================================================
# LLM Service Configuration
# =============================================================================

# Base URL for the LLM API endpoint
# Example: https://api.openai.com/v1 or https://api.deepseek.com
TARGET_BASE_URL=YOUR_BASE_URL

# API Key for the LLM service (SENSITIVE - keep this secret!)
TARGET_API_KEY=YOUR_API_KEY

# Model name to use for LLM requests
# Example: gpt-4o, deepseek-chat, etc.
TARGET_MODEL=YOUR_MODEL
